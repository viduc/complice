<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Complice{% endblock %}</title>
    {% block stylesheets %}
        <link
            rel="stylesheet"
            href="{{ asset('css/bootstrap.min.css') }}"
            type="text/css"
            media="screen"
        />
    {% endblock %}
</head>
<body>
{% include 'layout/navigation.html.twig' %}
{% block body %}
    <div class="row">
        <div class="col-lg-2">
            {% include 'layout/navigation-gauche.html.twig' %}
        </div>
        <div class="col-lg-10 container">
            {% block contenu %}{% endblock %}
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script
        type="text/javascript"
        src="{{ asset('js/jquery.3.5.1.min.js') }}" >
    </script>
    <script
        type="text/javascript"
        src="{{ asset('js/bootstrap.min.js') }}" >
    </script>
{% endblock %}
</body>
</html>



